//pattern program
public class Main
{
	public static void main(String[] args) {
	    int size=5;
	    for(int i=5;i>0;i--){
	        for(int j=0;j<i;j++){
	            System.out.print("*");
	        }
	        System.out.println();
	    }
	    for(int i=2;i<=5;i++){
	        for(int j=0;j<=i;j++){
	            System.out.print("*");
	        }
	        System.out.println();
	    }
	}
}
